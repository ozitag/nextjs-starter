{
  "name": "nextjs-starter",
  "version": "1.0.0",
  "private": true,
  "main": "server-build/index.js",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-server": "rimraf server-build && babel server -d server-build --extensions \".ts,.json\" ",
    "start-server": "node server-build/index.js",
    "build": "yarn build-server && next build",
    "start": "cross-env NODE_ENV=development yarn build-server && yarn start-server",
    "serve": "cross-env NODE_ENV=production yarn start-server",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "ci": "start-server-and-test start http://localhost:3000 cypress:run"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.4",
    "cookie": "^0.4.0",
    "express": "^4.17.1",
    "formik": "^2.1.4",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "next": "^9.3.4",
    "next-i18next": "^4.2.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "react": "^16.13.1",
    "react-app-polyfill": "1.0.6",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@cypress/webpack-preprocessor": "^4.1.3",
    "@storybook/react": "^5.3.14",
    "@svgr/webpack": "5.1.0",
    "@testing-library/cypress": "^6.0.0",
    "@types/express": "^4.17.3",
    "@types/js-cookie": "^2.2.5",
    "@types/lodash": "^4.14.149",
    "@types/node": "12.x",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "7.1.7",
    "@types/storybook__react": "^5.2.1",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.1",
    "cross-env": "^7.0.1",
    "cypress": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "6.x",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "fs-extra": "^8.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "next-compose-plugins": "^2.2.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.10.11",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
